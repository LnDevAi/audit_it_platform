{
  "name": "audit-platform-backend",
  "version": "1.0.0",
  "description": "Backend API pour la plateforme d'audit informatique E-DEFENCE",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "auto": "node scripts/start-automated.js start",
    "auto:check": "node scripts/start-automated.js check",
    "auto:ports": "node scripts/start-automated.js ports",
    "auto:db": "node scripts/start-automated.js db",
    "ports:configure": "node scripts/auto-configure-ports.js configure",
    "ports:resolve": "node scripts/auto-configure-ports.js resolve",
    "ports:status": "node scripts/auto-configure-ports.js status",
    "ports:restore": "node scripts/auto-configure-ports.js restore",
    "test:communication": "node scripts/test-communication.js test",
    "test:db": "node scripts/test-communication.js db",
    "test:backend": "node scripts/test-communication.js backend",
    "test:frontend": "node scripts/test-communication.js frontend",
    "test:performance": "node scripts/test-communication.js performance",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "security-check": "node ../scripts/security-check.js",
    "generate-secrets": "node ../scripts/generate-secrets.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run lint && npm run format:check && npm run test:coverage",
    "prepare": "husky install"
  },
  "keywords": [
    "audit",
    "security",
    "api",
    "express"
  ],
  "author": "E-DEFENCE",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "pdfkit": "^0.13.0",
    "prom-client": "^14.2.0",
    "openid-client": "^5.6.5",
    "stripe": "^12.18.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "qrcode": "^1.5.4",
    "redis": "^4.6.10",
    "sequelize": "^6.35.2",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-jest": "^27.9.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
